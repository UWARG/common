package TelemMessages;

struct Header {
    byte flag;
    byte length[2]; // uint16_t doesnt exist
    byte type;
}

struct SensorData {
    double latitude;
    double longitude;
    float altitude;
    float climb_rate;
    float track;
    float heading;
    float air_speed;
    float ground_speed;
    float roll;
    float pitch;
    float yaw;
    float roll_rate;
    float pitch_rate;
    float yaw_rate;
}
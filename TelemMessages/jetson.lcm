package messages;

struct JetsonOdometryData {
   Header header;
   SensorData sensorData;
   byte crc[4]; 
}

struct JetsonMovementRequest {
   Header header;
   boolean req;
   byte crc[4]; 
}

struct JetsonRelativeMovementCommand {
    Header header;
    float x;
    float y;
    float z;
    float heading;
    byte crc[4];
}

struct JetsonLandingInitiationCommand {
    Header header;
    boolean req;
    byte crc[4];
}